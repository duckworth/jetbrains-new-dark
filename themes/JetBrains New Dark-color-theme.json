{
	"$schema": "vscode://schemas/color-theme",
	"name": "JetBrains New Dark",
	"include": "./dark_modern.json",
	// custom syntax highlighting
	"tokenColors": [
	  {
		"scope": [
		  "variable.argument.css",
		  "punctuation.definition.tag",
		  "entity.name.tag",
		  "support.function.misc",
		  "meta.function.variable.css",
		  "meta.property-value.css support.function",
		  "meta.selector.css",
		  "entity.other.attribute-name.css"
		],
		"settings": {
		  "foreground": "#D5B778"
		}
	  },
	  {
		"scope": [
		  "keyword.other.unit",
		  "punctuation.separator.key-value.html",
		  "meta.tag.attributes keyword.operator.assignment"
		],
		"settings": {
		  "foreground": "#6AAB73"
		}
	  },
	  {
		"scope": [
		  "entity.other.attribute-name",
		  "support.type.property-name.css"
		],
		"settings": {
		  "foreground": "#BABABA"
		}
	  },
	  {
		"scope": ["support.type.property-name.json"],
		"settings": {
		  "foreground": "#C77DBB"
		}
	  },
	  {
		"scope": ["meta.tag.custom entity.name.tag.html"],
		"settings": {
		  "foreground": "#2fbaa3"
		}
	  },
	  {
		"scope": ["comment.block.documentation", "comment.block.javadoc"],
		"settings": {
		  "foreground": "#5F826B"
		}
	  },
	  {
		"scope": "entity.name.function.ruby",
		"settings": {
		  "foreground": "#CF8E6D"  // now amber for methods
		}
	  },
	  {
		"scope": "constant.language.symbol.ruby",
		"settings": {
		  "foreground": "#66A6FF"  // now blue for symbols
		}
	  },
	  {
		"scope": "entity.name.type.class.ruby",
		"settings": {
		  "foreground": "#ffc56d"  // now amber for class names
		}
	  },
	  {
		"scope": "entity.other.inherited-class.ruby",
		"settings": {
		  "foreground": "#fefffa"  // now light color for inherited classes
		}
	  },
	  {
		"scope": "variable.ruby",
		"settings": {
		  "foreground": "#d5af1e"  // color for ruby variables
		}
	  },
	  {
		"scope": "constant.language.symbol.hashkey.ruby",
		"settings": {
		  "foreground": "#66A6FF"  // color for ruby hash keys
		}
	  },
	  {
		"scope": "variable.parameter.function.ruby",
		"settings": {
		  "foreground": "#d5af1e"  // color for ruby function parameters
		}
	  },
	  {
		"scope": [
			"entity.name.function.member",
			"entity.name.function",
			"support.function",
			"support.constant.handlebars",
			"source.powershell variable.other.member",
			"entity.name.operator.custom-literal"
		],
		"settings": {
			"foreground": "#BABABA"  // changing method color to light gray
		}
	  }
	]
  }
